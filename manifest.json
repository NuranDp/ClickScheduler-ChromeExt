{
    "manifest_version": 2,
    "name": "Click Schedule",
    "description": "Be able to click button on schedule",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
    ]
}